    "                if event.content and event.content.parts:\n",
    "                    for part in event.content.parts:\n",
    "                        if part.text:\n",
    "                            print(part.text, end=\"\", flush=True)\n",
    "        \n",
    "        print(\"\\n\")\n",
    "\n",
